{
    "name": "bmonzon/cordial-sdk",
    "description": "The unoffical Cordial PHP SDK",
    "require": {
        "guzzlehttp/guzzle": "^7.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.4|^9.0"
    },
    "authors": [
        {
            "name": "Bryan Monzon",
            "email": "bmonzon@cordial.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Cordial\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
